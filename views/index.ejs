<%- include('./partials/header.ejs') %>
    <h1>Here is Your Library</h1>
    <ul>
        <% for(let i = 0; i < allBooks.length; i++) { %>
            <li>
                <a href="/mylibrary/<%=allBooks[i].id%>">
                    <%=allBooks[i].title%>
                </a>
                <img src="https://covers.openlibrary.org/b/isbn/<%=allBooks[i].isbn%>-L.jpg" alt=""/>
            </li>
        <% } %>
    </ul>
</body>
